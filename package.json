{
  "name": "firebase-api-surface",
  "version": "0.8.5",
  "description": "typings for the older parts of google SDK that don't have good typing",
  "license": "MIT",
  "repository": "git@github.com:forest-fire/firebase-api-surface.git",
  "author": "Ken Snyder<ken@ken.net>",
  "keywords": [
    "typescript",
    "firebase"
  ],
  "files": [
    "dist"
  ],
  "main": "dist/firebase-api-surface.cjs.js",
  "module": "dist/esnext/index.js",
  "typings": "dist/esnext/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "build": "tslint src/**/*.ts && tsc -P tsconfig.esnext.json && rollup -c rollup.umd.js && rollup -c rollup.cjs.js",
    "test": "ts-node ./scripts/test.ts ",
    "coverage": "nyc --reporter=text --reporter=html mocha --compilers ts:ts-node/register",
    "deploy": "ts-node ./scripts/deploy.ts "
  },
  "dependencies": {
    "common-types": "^1.7.36",
    "@firebase/app-types": "^0.1.1",
    "@firebase/database-types": "^0.1.1"
  },
  "devDependencies": {
    "@types/rimraf": "^2.0.2",
    "@types/shelljs": "^0.7.7",
    "async-shelljs": "^0.1.2",
    "bili": "^3.1.2",
    "chalk": "^2.3.0",
    "js-yaml": "^3.10.0",
    "lodash.first": "^3.0.0",
    "lodash.last": "^3.0.0",
    "rimraf": "^2.6.2",
    "rollup": "^0.60.1",
    "test-console": "^1.1.0",
    "tslint": "^5.9.1",
    "tslint-config-prettier": "^1.8.0",
    "typescript": "^3.0.0-dev.20180606"
  }
}
